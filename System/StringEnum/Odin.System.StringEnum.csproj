  <ItemGroup>
    <ProjectReference Include="..\Result\Odin.System.Result.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="" />
  </ItemGroup>
</Project>