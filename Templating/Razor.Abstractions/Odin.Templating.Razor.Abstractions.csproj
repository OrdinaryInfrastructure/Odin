<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0;net10.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <RootNamespace>Odin.Templating.Razor</RootNamespace>
    <ImplicitUsings>true</ImplicitUsings>
    <Description>Provides OrDINary use case rendering of embedded '.cshtml' templates using Razor syntax outside 
      the context of ASP.Net, via IRazorTemplateRenderer.RenderAsync-TModel(string templateKey, TModel viewModel)
    For example, rendering email body HTML from an HTML template.
    </Description>
    <WarningsAsErrors>1591;1573;</WarningsAsErrors> <!-- Not to be removed. Documentation is required. -->
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\System\Result\Odin.Result.csproj" />
  </ItemGroup>
</Project>