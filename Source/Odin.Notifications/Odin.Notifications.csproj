<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>Odin.Notifications</RootNamespace>
    <PackageId>Odin.Notifications</PackageId>
    <Authors>Mark Derman</Authors>
    <Company>Soulv Software (Pty) Ltd</Company>
    <Description></Description>
    <Copyright>Copyright (c) Soulv Software 2022</Copyright>
    <PackageProjectUrl>https://soulv.co.za</PackageProjectUrl>
    <RepositoryUrl>https://dev.azure.com/soulvsoftware/Home/_git/Odin</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <WarningLevel>3</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <NoWarn></NoWarn>
    <DocumentationFile>bin\Debug\net8.0\Odin.Notifications.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <WarningLevel>3</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn></NoWarn>
    <DocumentationFile>bin\Release\net8.0\Odin.Notifications.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Odin.Common\Odin.Common.csproj" />
    <ProjectReference Include="..\Odin.Email\Odin.Email.csproj" />
  </ItemGroup>
</Project>